@model IEnumerable<FilmDukkaniMvc.AylikParaKesimRelation>

@{
    ViewBag.Title = "Faturalari Kes";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2><a href="@Url.Action("FaturaUcretleriAl")">Bütün Faturaları Kes </a></h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Kullanıcı Adı")
        </th>
        <th>
            @Html.DisplayName("Kesilecek Para")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Musteriler.Adi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KesilenPara)
            </td>

        </tr>
    }

</table>
